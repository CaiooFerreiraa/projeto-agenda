<% if (error.length > 0) { %>
  <div class="row">
    <div class="col my-3">
      <div class="alert alert-danger">
        <% error.forEach(erro => { %>
         <%= erro %><br>
        <% }) %>
      </div>
    </div>
  </div>
<% } else if (success.length > 0) {%>
  <div class="row">
    <div class="col my-3">
      <div class="alert alert-success">
        <% success.forEach(ok => { %>
         <%= ok %><br>
        <% }) %>
      </div>
    </div>
  </div>
<% } %>
 